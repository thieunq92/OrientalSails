<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="CMS.Web.AdminArea.DAL.Domain"
                   assembly="CMS.Web" auto-import="false">
	<class name="AnswerOption" table="sv_AnswerOption" batch-size="10">
		<cache usage="read-write"/>
		<id name="Id">
			<generator class="native" />
		</id>
		<property name="Option" column="`Option`"/>
		<property name="QuestionId"/>
		<property name="AnswerSheetId"/>
	</class>
</hibernate-mapping>
