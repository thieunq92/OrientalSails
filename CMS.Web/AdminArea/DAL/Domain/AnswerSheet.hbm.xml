<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="CMS.Web.AdminArea.DAL.Domain"
                   assembly="CMS.Web" auto-import="false">
	<class name="AnswerSheet" table="sv_AnswerSheet" batch-size="10">
		<cache usage="read-write"/>
		<id name="Id">
			<generator class="native" />
		</id>
		<property name="Name"/>
		<property name="IsSent" column="IsSentEmail"/>
		<property name="Address"/>
		<property name="Email"/>
		<property name="Date"/>
		<property name="Deleted"/>
		<property name="Guide"/>
		<property name="Driver"/>
		<property name="RoomNumber"/>
		<property name="CruiseId"/>
		<property name="ROOM_ID"/>
		<property name="LOAI_FEEDBACK"/>
		<property name="SO_DIEN_THOAI"/>
	</class>
</hibernate-mapping>
